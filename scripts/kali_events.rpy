    q "Quem você acha que construiu aqueles cachorros robôs? Escravos!"

# game/scripts/kali_events.rpy:5459
translate pt_br kaliuni_0bd5553d:

    # q "Ohhh..."
    q "Ohhh..."

# game/scripts/kali_events.rpy:5461
translate pt_br kaliuni_1e029caa:

